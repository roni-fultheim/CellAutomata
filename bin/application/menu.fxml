<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="300" prefWidth="350" spacing="30" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reversiap.MenuController">
   <children>
      <Button fx:id="startGameBtn" mnemonicParsing="false" onAction="#startGame" prefHeight="50.0" prefWidth="170.0" text="Start Game" textAlignment="CENTER" />
      <Button fx:id="settingsBtn" mnemonicParsing="false" onAction="#changeSettings" prefHeight="50.0" prefWidth="170.0" text="Settings" />
      <Button fx:id="exitBtn" mnemonicParsing="false" prefHeight="50.0" prefWidth="170.0" text="Exit" />
   </children>
</VBox>
